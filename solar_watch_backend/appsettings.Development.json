{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    },
    "Debug": {
      "LogLevel": {
        "Default": "Information"
      }
    }
  },
  "File": {
    "Path": "logs/app.log",
    "Append": true,
    "MaxFileSize": 10485760,
    "MaxRollingFiles": 3,
    "LogDirectory": "logs"
  }
}