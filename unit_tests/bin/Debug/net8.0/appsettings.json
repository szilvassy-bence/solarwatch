{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "File": {
    "Path": "logs/app.log",
    "Append": true,
    "MaxFileSize": 10485760,
    "MaxRollingFiles": 3,
    "LogDirectory": "logs"
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "SolarWatch": "Database=SolarWatchDb;User Id=sa;Encrypt=false"
  },
  "TokenValidation": {
    "ValidIssuer": "SolarWatchWithAuthBackend",
    "ValidAudience": "SolarWatchWithAuthBackend"
  }
}
